document.addEventListener('DOMContentLoaded', (event) => {
const today = new Date().toISOString().split('T')[0];
document.getElementById('datefrom').setAttribute('min', today);
});

document.addEventListener('DOMContentLoaded', (event) => {
const today = new Date().toISOString().split('T')[0];
document.getElementById('dateto').setAttribute('min', today);
});